    "#print fibonacci sequence.\n",
    "n=int(input(\"Enter the number\"))\n",
    "num1=0\n",
    "num2=1\n",
    "next_num=num2\n",
    "count=1\n",
    "while count <=n:\n",
    "    print(next_num,end=\" \")\n",
    "    count += 1\n",
    "    num1,num2=num2,next_num\n",
    "    next_num=num1+num2\n",
    "print()"
